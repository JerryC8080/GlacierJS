import { LifecycleLog } from '@pnpm/core-loggers';
import * as Rx from 'rxjs';
declare const _default: (log$: {
    lifecycle: Rx.Observable<LifecycleLog>;
}, opts: {
    appendOnly?: boolean;
    aggregateOutput?: boolean;
    cwd: string;
    width: number;
}) => Rx.Observable<Rx.Observable<{
    msg: string;
}>>;
export default _default;
declare function aggregateOutput(source: Rx.Observable<LifecycleLog>): Rx.Observable<LifecycleLog>;
