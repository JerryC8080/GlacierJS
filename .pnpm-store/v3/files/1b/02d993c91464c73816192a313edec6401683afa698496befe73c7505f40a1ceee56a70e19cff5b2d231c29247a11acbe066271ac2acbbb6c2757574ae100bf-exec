import { Reflection } from 'typedoc';
declare namespace reflectionSourceUtils {
    const getReflectionSourceFileName: (reflection?: Reflection | undefined) => string | undefined;
    const getPageSourceCoordinates: (reflection: Reflection | undefined, position: number) => {
        line: number;
        column: number;
        file: string;
    } | undefined;
    const getSourceLocationBestClue: (reflection?: Reflection | undefined, position?: number | undefined) => string;
}
export default reflectionSourceUtils;
